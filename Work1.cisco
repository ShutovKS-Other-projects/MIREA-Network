#Switch1
#Switch Start 
enable
configure terminal
hostname KShutov-Switch-1

#Created VLAN 
vlan 10
name KShutov-VLAN-10
vlan 20
name KShutov-VLAN-20
vlan 30
name KShutov-VLAN-30
vlan 40
name KShutov-VLAN-40
vlan 50
name KShutov-VLAN-50
vlan 60
name KShutov-VLAN-60
vlan 70
name KShutov-Management

#Created interface
interface e0/2
switchport mode access
switchport access vlan 10
description "KShutov-PC-12"
interface e0/1
switchport mode access
switchport access vlan 10
description "KShutov-PC-13"
interface e1/0
switchport mode access
switchport access vlan 20
description "KShutov-PC-34"
interface e0/0
switchport trunk encapsulation dot1q
switchport mode trunk
description "To KShutov-Switch-2"
interface e1/1
switchport mode access
switchport access vlan 70
description "KShutov-SSH-Client"

interface vlan 70
description "KShutov-Management"
ip address 10.31.70.1 255.255.255.0
no shutdown
exit
ip route 0.0.0.0 0.0.0.0 10.31.70.254

#Setting VTP server
vtp domain KShutov-Server
vtp password 12345
vtp version 3
vtp mode server
exit
vtp primary vlan

#Save
wr

