#######

#Main-Router
#no

#Router Start
en
conf t
hostname KShutov-Main-Router
no service config
no ip domain lookup

#Router interface enable 
int e0/0
no shutdown

#created subinterface for VLAN10-60 and setting DHCP server
int e0/0.10
encapsulation dot1q 10
ip address 10.31.10.254 255.255.255.0
ip dhcp pool KShutov-VLAN10POOL
network 10.31.10.0 255.255.255.0
default-router 10.31.10.254
dns-server 8.8.8.8 8.8.4.4
exit
ip dhcp excluded-address 10.31.10.1 10.31.10.9
ip dhcp excluded-address 10.31.10.101 10.31.10.254

int e0/0.20
encapsulation dot1q 20
ip address 10.31.20.254 255.255.255.0
